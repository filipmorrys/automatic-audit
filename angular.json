{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "automatic-audit": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/automatic-audit",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "tmstmm": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.tmstmm.ts"
                }
              ]
            },
            "tmsint": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.tmsint.ts"
                }
              ]
            },
            "irlint": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.irlint.ts"
                }
              ]
            },
            "irltmm": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.irltmm.ts"
                }
              ]
            },
            "irllocal": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.irllocal.ts"
                }
              ]
            },
            "tmslocal": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.tmslocal.ts"
                }
              ]
            },
            "estint": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environments.ts",
                  "with": "src/app/environments/environments.estint.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "automatic-audit:build:production"
            },
            "tmstmm": {
              "browserTarget": "automatic-audit:build:tmstmm",
              "proxyConfig": "proxy.conf.tmstmm.json"
            },
            "tmsint": {
              "browserTarget": "automatic-audit:build:tmsint",
              "proxyConfig": "proxy.conf.tmsint.json"
            },
            "estint": {
              "browserTarget": "automatic-audit:build:estint",
              "proxyConfig": "proxy.conf.estint.json"
            },
            "irltmm": {
              "browserTarget": "automatic-audit:build:irltmm",
              "proxyConfig": "proxy.conf.irltmm.json"
            },
            "irlint": {
              "browserTarget": "automatic-audit:build:irlint",
              "proxyConfig": "proxy.conf.irlint.json"
            },
            "irllocal": {
              "browserTarget": "automatic-audit:build:irllocal",
              "proxyConfig": "proxy.conf.irllocal.json"
            },
            "tmslocal": {
              "browserTarget": "automatic-audit:build:tmslocal",
              "proxyConfig": "proxy.conf.tmslocal.json"
            }

          },
          "defaultConfiguration": "tmstmm"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "automatic-audit:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
